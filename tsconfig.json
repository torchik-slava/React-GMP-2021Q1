{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  }
}
